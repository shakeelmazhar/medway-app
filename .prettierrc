{
    "bracketSpacing": true,
    "bracketSameLine": false,
    "singleQuote": false,
    "trailingComma": "all",
    "tabWidth": 2,
    "semi": true,
    "plugins": [ "@ianvs/prettier-plugin-sort-imports" ],
    "importOrder": [
        "^react$",
        "^react-native$",
        "^react-native/(.*)$",
        "^@react-navigation/(.*)$",
        "<THIRD_PARTY_MODULES>",
        "^@(.*)$",
        "^[./]"
    ],
    "importOrderParserPlugins": [ "typescript", "jsx", "decorators-legacy" ],
    "importOrderTypeScriptVersion": "5.0.0"
}

